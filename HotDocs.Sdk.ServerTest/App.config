          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="500000" maxBytesPerRead="4096"
                        maxNameTableCharCount="16384" />
          <reliableSession ordered="true" inactivityTimeout="00:10:00" enabled="false" />
          <security mode="Message">
            <transport clientCredentialType="Windows" proxyCredentialType="None" realm="" />
            <message clientCredentialType="Windows" negotiateServiceCredential="true" algorithmSuite="Default" />
          </security>
        </binding>
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:55383/HDSvc.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IHDSvc" contract="HotDocs.Sdk.Server.Contracts.IHDSvc"
                name="BasicHttpBinding_IHDSvc" />
      <endpoint address="http://localhost:55383/HDSvc.svc/secure" binding="wsHttpBinding"
                bindingConfiguration="WSHttpBinding_IHDSvc" contract="HotDocs.Sdk.Server.Contracts.IHDSvc"
                name="WSHttpBinding_IHDSvc">
        <identity>
          <userPrincipalName value="fjerstadj@hotdocs.dev" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Net.Http" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.2.29.0" newVersion="2.2.29.0" />
